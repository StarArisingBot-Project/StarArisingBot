		<PackageReference Include="System.Drawing.Common" Version="6.0.2-mauipre.1.22054.8" />
	</ItemGroup>

	<!--Important Files-->
	<ItemGroup>
		<None Update="SAB.System\Files\BotToken.json" CopyToOutputDirectory="PreserveNewest" />
	</ItemGroup>

	<!--Folders-->
	<ItemGroup>
	  <Folder Include="SAB.Bot\SAB.Bot.Commands\Commands\Minigames\" />
