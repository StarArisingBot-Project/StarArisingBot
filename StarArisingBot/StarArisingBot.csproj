    <OutputType>Exe</OutputType>
    <TargetFramework>net5.0</TargetFramework>
    <AssemblyName>StarArisingBot</AssemblyName>
    <RootNamespace>SAB</RootNamespace>
    <StartupObject>SAB.System.Program</StartupObject>
