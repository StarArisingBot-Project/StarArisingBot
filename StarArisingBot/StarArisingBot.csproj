		<PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
		<PackageReference Include="Starciad.SABMinigameEngine" Version="1.0.3" />
		<PackageReference Include="System.Drawing.Common" Version="6.0.2-mauipre.1.22054.8" />
	</ItemGroup>

	<!--Projects-->
	<ItemGroup>
	  <ProjectReference Include="..\StarArisingBot.Business\StarArisingBot.Business.csproj" />
	</ItemGroup>
	
	<ItemGroup>
	  <Folder Include="SAB.Managers\Instances\" />
